        }
      }
    },
    "interfacedefs": {
      "type": "object"
    },
    "calls": {
      "type": "object",
      "patternProperties": {
        "\\w+": {
          "type": "object",
          "properties": {
            "evcmd": {},
            "tasks": {},
            "variables": {},
            "t_variables": {}
          },
          "required": ["evcmd", "tasks", "variables", "t_variables"]
        }
      }
    }
  },
  "additionalProperties": true,
  "required": [ "components", "interfacedefs", "calls"]
}
